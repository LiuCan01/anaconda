#: pyanaconda/iutil.py:777
msgid "No host url"
msgstr "Nema url-a domaćina"

#: pyanaconda/iutil.py:798
msgid "malformed URL, cannot parse it."
msgstr "deformiran URL, parsiranje nemoguće."

#: pyanaconda/iutil.py:814
msgid "URL has no host component"
msgstr "URL nema komponente domaćina"

#: pyanaconda/kickstart.py:165
#, python-format
msgid "Escrow certificate %s requires the network."
msgstr "Escrow certifikat %s zahtijeva pristup mreži."

#: pyanaconda/kickstart.py:173
#, python-format
msgid ""
"The following error was encountered while downloading the escrow "
"certificate:\n"
"\n"
"%s"
msgstr ""
"Tijekom preuzimanja escrow certifikata došlo je do pogreške:\n"
"\n"
"%s"

#: pyanaconda/kickstart.py:247
#, python-format
msgid "%s is missing. Cannot setup authentication."
msgstr "%s nedostaje. Nemoguće podesiti autentifikaciju."

#: pyanaconda/kickstart.py:272
#, python-format
msgid "autopart fstype of %s is invalid."
msgstr "autodio fstype od %s je nevažeći."

#: pyanaconda/kickstart.py:289
#, python-format
msgid "Settings default fstype to %s failed."
msgstr "Postavke fstype na %s neuspjele."

#: pyanaconda/kickstart.py:322
msgid "GRUB2 does not support installation to a partition."
msgstr "GRUB2 ne podržava instalaciju na particiju."

#: pyanaconda/kickstart.py:415
#, python-format
msgid "More than one match found for given boot drive \"%s\"."
msgstr "Više od jednog rezultata za dani boot pogon \"%s\"."

#: pyanaconda/kickstart.py:420
#, python-format
msgid "Requested boot drive \"%s\" doesn't exist or cannot be used."
msgstr "Traženi boot pogon \"%s\" ne postoji ili ne može biti korišten"

#: pyanaconda/kickstart.py:494
#, python-format
msgid ""
"Btrfs partition \"%(device)s\" has a format of \"%(format)s\", but should "
"have a format of \"Btrfs\"."
msgstr ""
"BTRFS particija \"%(device)s\" je formata \"%(format)s\", ali bi trebala "
"biti formata \"Btrfs\"."

#: pyanaconda/kickstart.py:499
#, python-format
msgid "Tried to use undefined partition \"%s\" in Btrfs volume specification."
msgstr ""
"Pokušaj korištenja nedefinirane particije \"%s\" u BTRFS volume "
"specifikaciji."

#: pyanaconda/kickstart.py:512
msgid ""
"Btrfs volume defined without any member devices.  Either specify member "
"devices or use --useexisting."
msgstr ""
"BTRFS volumen definiran bez uređaja članova. Ili specificirajte uređaje "
"članove ili koristite -useexisting."

#: pyanaconda/kickstart.py:521 pyanaconda/kickstart.py:892
#: pyanaconda/kickstart.py:1481
#, python-format
